<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dao.tables" auto-import="true">
    <class name="Orders">
        <!-- 主键 ，映射-->
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="streamNumber" column="streamNumber" />
        <property name="dishType" column="dishType" type="string" />
        <property name="dishNumber" column="dishNumber" />
        <property name="dishName" column="dishName" type="string" />
        <property name="isOrdered" column="isOrdered" />
        <property name="isGifts" column="isGifts" />
        <property name="isBack" column="isBack" />
        <property name="price" column="price" />
        <property name="discount" column="discount" />
        <property name="producterId" column="producterId" />
    </class>
</hibernate-mapping>
