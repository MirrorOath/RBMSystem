<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dao.tables" auto-import="true">
    <class name="MProvider">
        <!-- 主键 ，映射-->
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="name" column="name" type="string" />
        <property name="mName" column="mName" type="string" />
        <property name="address" column="address" type="string" />
        <property name="phone" column="phone" type="string" />
        <property name="fax" column="fax" type="string" />
        <property name="cMan" column="cMan" type="string"/>
        <property name="cManMobile" column="cManMobile" type="string" />
        <property name="contractId" column="contractId" />
    </class>
</hibernate-mapping>
