<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dao.tables" auto-import="true">
    <class name="TableEnds">
        <!-- 主键 ，映射-->
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="streamId" column="streamId" />
        <property name="date" column="date" type="timestamp" />
        <property name="typeOfCoin" column="typeOfCoin" />
        <property name="nameOfCoin" column="nameOfCoin" />
        <property name="shouldPay" column="shouldPay" />
        <property name="payActually" column="payActually" />
        <property name="convertToRMB" column="convertToRMB" type="string" />
        <property name="changeMoney" column="changeMoney" type="string" />
        <property name="payeeId" column="payeeId" />
    </class>
</hibernate-mapping>
