<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="dao.tables" auto-import="true">
    <class name="Checkout">
        <!-- 主键 ，映射-->
        <id name="id" column="id">
            <generator class="increment"/>
        </id>
        <property name="seatRotationId" column="seatRotationId" />
        <property name="date" column="date" type="timestamp" />
        <property name="paymentTypes" column="paymentTypes" />
        <property name="currency" column="currency" />
        <property name="amountsPayable" column="amountsPayable" />
        <property name="actuallyPaid" column="actuallyPaid" />
        <property name="convertToRMB" column="convertToRMB" />
        <property name="change" column="change" />
        <property name="payeeId" column="payeeId" />
    </class>
</hibernate-mapping>
